{
    "name": "vue3-element-inspector",
    "short_name": "vei",
    "description": "It adds a sidebar tab in the elements inspection panel that shows the currently selected elements Vue properties.",
    "version": "1.0.0",
    "manifest_version": 2,
    "default_locale": "en",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "minimum_chrome_version": "10.0",
    "devtools_page": "pages/devtools.html",
    "permissions": [
        "bookmarks",
        "clipboardRead",
        "clipboardWrite",
        "commands",
        "contentSettings",
        "contextMenus",
        "cookies",
        "debugger",
        "declarativeContent",
        "history",
        "input",
        "management",
        "notifications",
        "pageCapture",
        "storage",
        "proxy",
        "tabCapture",
        "tabs",
        "topSites",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
