{
    "name": "vue3-element-inspector",
    "private": true,
    "version": "1.0.0",
    "description": "It adds a sidebar tab in the elements inspection panel that shows the currently selected elements Vue properties.",
    "scripts": {
        "start": "npm run dev:chrome",
        "build": "npm run build:chrome",
        "build:chrome": "gulp pack --production --vendor=chrome",
        "build:firefox": "gulp pack --production --vendor=firefox",
        "build:opera": "gulp pack --production --vendor=opera",
        "build:edge": "gulp pack --production --vendor=edge",
        "dev": "npm run dev:chrome",
        "dev:chrome": "gulp --watch --vendor=chrome",
        "dev:firefox": "gulp --watch --vendor=firefox",
        "dev:opera": "gulp --watch --vendor=opera",
        "dev:edge": "gulp --watch --vendor=edge"
    },
    "standard": {
        "globals": [
            "chrome"
        ],
        "parser": "babel-eslint"
    },
    "devDependencies": {
        "@types/chrome": "^0.x.x",
        "@types/fs-extra": "^9.0.13",
        "@types/glob": "^7.1.4",
        "@types/gulp": "^4.0.9",
        "@types/jquery": "^3.5.6",
        "@types/lodash": "^4.14.175",
        "@types/mini-css-extract-plugin": "^1.2.2",
        "@types/sjcl": "^1.0.30",
        "@types/webpack": "4.41.31",
        "babel-cli": "6.x.x",
        "babel-core": "6.x.x",
        "babel-eslint": "^10.x.x",
        "babel-loader": "8.x.x",
        "babel-preset-env": "1.x.x",
        "babili-webpack-plugin": "0.x.x",
        "chromereload": "0.x.x",
        "css-loader": "^5.2.0",
        "del": "5.x.x",
        "fs-extra": "^10.0.0",
        "glob": "^7.2.0",
        "gulp": "4.x.x",
        "gulp-bump": "3.x.x",
        "gulp-cache": "1.x.x",
        "gulp-clean-css": "^4.x.x",
        "gulp-copy": "^4.0.1",
        "gulp-filter": "6.x.x",
        "gulp-git": "2.x.x",
        "gulp-if": "3.x.x",
        "gulp-imagemin": "7.x.x",
        "gulp-json-transform": "0.x.x",
        "gulp-less": "4.x.x",
        "gulp-livereload": "4.x.x",
        "gulp-plumber": "1.x.x",
        "gulp-sass": "^4.x.x",
