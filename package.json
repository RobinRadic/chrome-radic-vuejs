        "require-dir": "1.x.x",
        "sass": "^1.42.1",
        "sass-loader": "^10.1.0",
        "sjcl": "^1.0.8",
        "standard": "14.x.x",
        "style-loader": "^2.0.0",
        "ts-loader": "^6.x.x",
        "tslint": "^6.x.x",
        "typescript": "^4.x.x",
